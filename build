#!/bin/bash

source vdev/conf/vm
source vdev/conf/paths
source vdev/detail/utils


target=$1
configuration=$2

vdev/detail/execute "$guest_name" "$user" "$password" "$guest_build_bat" "$vs_version" "$guest_solution_path" "$target" "$configuration" "$guest_build_log_path"
retval=$?
printf '\n'

exit $retval

