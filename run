#!/bin/bash

export working_dir=$(pwd)
cd $(dirname $0)


source "$working_dir/vdevrc/vm"
source "$working_dir/vdevrc/paths"
source detail/utils

target=$1
configuration=$2

properties_path="$working_dir/vdevrc/$target-$configuration"
detail/loadprojprops "$working_dir/$solution_path" "$target" "$configuration" "$properties_path"
source "$properties_path"

binary_name="$name$extension"
detail/"$vm"/execute "$guest_run_bat" "$guest_solution_dir" "$out_dir" "$binary_name"
retval=$?
printf "\n" # if put newline in Windows then it can be swallowed

exit $retval

