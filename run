#!/bin/bash

source conf/vm
source conf/paths
source detail/utils

target=$1
configuration=$2

properties_path="conf/$target-$configuration"
detail/loadprojprops "$solution_path" "$target" "$configuration" "$properties_path"
source "$properties_path"

binary_name="$name$extension"
detail/"$vm"/execute "$guest_run_bat" "$guest_solution_dir" "$out_dir" "$binary_name"
retval=$?
printf "\n" # if put newline in Windows then it can be swallowed

exit $retval

